{
  "name": "@srclaunch/validation",
  "description": "Form/value/etc validation library used in AppLab applications and services",
  "author": "Steven Bennett <steven@srclaunch.com>",
  "license": "MIT",
  "version": "0.1.2",
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "package.json",
    "dist"
  ],
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "scripts": {
    "build": "yarn clean && yarn build:applab",
    "build:applab": "applab build",
    "clean": "rm -fr ./dist",
    "git:push": "git push --follow-tags origin main",
    "qr": "yarn build && git add . && git commit -m \"Quick release - patch\" && yarn release",
    "release": "yarn test && yarn version:patch && yarn git:push",
    "test": "ava",
    "test:coverage": "yarn c8 ava",
    "test:watch": "yarn ava --watch",
    "version:patch": "standard-version",
    "yui": "yarn upgrade-interactive"
  },
  "devDependencies": {
    "@srclaunch/cli": "^0.1.92",
    "@srclaunch/dx": "^0.1.18",
    "@srclaunch/types": "^0.1.28",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "ava": "^4.2.0",
    "c8": "^7.11.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-stylelint": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-sort-requires": "^2.1.0",
    "eslint-plugin-testing-library": "^5.3.1",
    "eslint-plugin-unicorn": "^42.0.0",
    "prettier": "^2.6.2",
    "prettier-eslint": "^13.0.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.6.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "email-validator": "^2.0.4",
    "js-file-download": "^0.4.12",
    "password-validator": "^5.2.1",
    "zxcvbn": "^4.4.2"
  }
}
